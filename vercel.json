{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      }
    ]
  }
  